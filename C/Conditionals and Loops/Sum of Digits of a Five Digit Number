#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,temp,sum=0,remainder;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    temp=n;
    while(temp != 0)
    {
        remainder=temp % 10;
        sum= sum + remainder;
        temp = temp / 10;
    }
    
    printf("%d\n",sum);
    
    return 0;
}
